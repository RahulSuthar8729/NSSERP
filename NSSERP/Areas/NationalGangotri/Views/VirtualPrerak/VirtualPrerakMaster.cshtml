
@{
    ViewData["Title"] = "VirtualPrerakMaster";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!DOCTYPE html>
<html>
<head>
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" integrity="..." crossorigin="anonymous" />
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="..."></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="..."></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="..."></script>
    <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
</head>
<body>
    <div class="card">
        <div class="card-header">
            <div class="row">
                <div class="col text-left">
                    <h5>
                        <span style="color:#1cb8a1">VIRTUAL PRERAK</span>
                        <span style="color:#28166f">MASTER</span>
                    </h5>
                </div>
                <div class="col text-left">
                    <h5>
                        <span style="color:#1cb8a1">INDIAN GANGOTRI</span>
                    </h5>
                </div>
                <div class="col-2 text-right">
                    <a href="/Gangotri/Home" title="Go Back Home" class="btn btn-warning">
                        <i class="fas fa-arrow-left"></i> Back
                    </a>


                </div>
            </div>
        </div>
        <div class="card-body">
            <form class="row-cols-lg-12">
                <div class="row">
                    <div class="form-group col-lg-3">
                        <label for="">Prerak ID</label>
                        <input class="form-control" id="" type="text" name="" />
                    </div>
                    <div class="form-group col-lg-3">
                        <label for="responsefrom">Category</label>
                        <select class="form-control" id="event" name="event">
                            <option value="0">select</option>
                            <option value="event">event 2</option>
                            <option value="event">event 3</option>
                        </select>
                    </div>
                    <div class="form-group col-lg-3">
                        <label for="" class="mr-2">Name</label>
                        <div class="d-flex">
                            <select class="form-control col-lg-3 mr-2" id="" name="">
                                <option value="0"><></></option>
                                <option value="">Shree</option>
                                <option value="">Mrs.</option>
                            </select>
                            <input type="text" class="form-control col-lg-7" id="" name="">
                        </div>
                    </div>
                    <div class="form-group col-lg-3">
                        <label for="">Address</label>
                        <textarea class="form-control" id="" rows="2" name=""></textarea>

                    </div>
                    <div class="form-group col-lg-3">
                        <label for="">Company</label>
                        <input class="form-control" id="" type="text" name="" />
                    </div>

                    <div class="form-group col-lg-3">
                        <label for="">City</label>
                        <select class="form-control" id="event" name="event">
                            <option value="0">select</option>
                            <option value="event">Udaipur</option>
                            <option value="event">Bhilwara</option>
                        </select>
                    </div>
                    <div class="form-group col-lg-3">
                        <label for="">STD Code</label>
                        <input class="form-control" id="" type="text" name="" />
                    </div>
                    <div class="form-group col-lg-3">
                        <label for="">Phone No.(Resi)</label>
                        <input class="form-control" id="" type="text" name="" />
                    </div>
                    <div class="form-group col-lg-3">
                        <label for="">Mobile No.</label>
                        <input class="form-control" id="" type="text" name="" />
                    </div>
                    <div class="form-group col-lg-3">
                        <label for="">Email ID</label>
                        <input class="form-control" id="" type="text" name="" />
                    </div>
                    <div class="form-group col-lg-3">
                        <label for="">Phone No.(Office)</label>
                        <input class="form-control" id="" type="text" name="" />
                    </div>
                    <div class="form-group col-lg-3">
                        <label for="">Fax No.</label>
                        <input class="form-control" id="" type="text" name="" />
                    </div>
                 
                </div>
            </form>
        </div>
    </div>
    <br />
    <div class="container-lg">
        <div class="text-center">
            <button type="submit" class="btn btn-success"><i class="fas fa-check"></i> Submit</button>           
        </div>
    </div>

    <br />
    <br />
    <br />

</body>
</html>

